    "typedoc": "^0.26.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "geostyler-cql-parser": "^3.0.2",
